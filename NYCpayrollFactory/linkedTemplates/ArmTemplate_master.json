{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "NYCpayrollFactory"
		},
		"LinkService_Azure_DL_Files_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LinkService_Azure_DL_Files'"
		},
		"LinkService_SQL_db_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LinkService_SQL_db'"
		},
		"LinkService_Synapse_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LinkService_Synapse'"
		},
		"LinkService_Azure_DL_Files_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlsnycpayrollerono.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "NYCpayrollFactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LinkService_Azure_DL_Files_accountKey": {
						"value": "[parameters('LinkService_Azure_DL_Files_accountKey')]"
					},
					"LinkService_SQL_db_connectionString": {
						"value": "[parameters('LinkService_SQL_db_connectionString')]"
					},
					"LinkService_Synapse_connectionString": {
						"value": "[parameters('LinkService_Synapse_connectionString')]"
					},
					"LinkService_Azure_DL_Files_properties_typeProperties_url": {
						"value": "[parameters('LinkService_Azure_DL_Files_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}